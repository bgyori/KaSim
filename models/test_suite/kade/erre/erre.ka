%agent: E(r)
%agent: R(e,r)
%agent: A(x)
%agent: B(x)

E(r[.]),R(e[.]) -> E(r[1]),R(e[1]) @ 1
E(r[1]),R(e[1],r[.]) -> E(r[.]),R(e[.],r[.]) @ 2
R(e[_],r[.]),R(e[_],r[.]) -> R(e[_],r[1]),R(e[_],r[1]) @ 3
R(r[_]) -> . @ [T]
R(r[_]) -> R(r[.]) @ 5
. -> R() @ 6
A(x[.]), B(x[.]) <-> A(x[1]),B(x[1]) @ 12,13

%obs: 'dimmers' |R(r[_])|/2
%obs: 'monomers' |R(r[.])|

%init: 10 E(r[1]),R(e[1],r[2]),R(r[2],e[3]),E(r[3])
%init: 10 E(r[.])
%init: 100 R(r[.],e[.])
%init: 4.5 A()
%init: 6 B()
