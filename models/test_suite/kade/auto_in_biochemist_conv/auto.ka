%agent: E(r)
%agent: R(e,r)
%agent: A(x)
%agent: B(x)

R(e[_],r[.]),R(e[_],r[.]) <-> R(e[_],r[1]),R(e[_],r[1]) @ 3,4
A(),A(),B(),B(),B() -> .,.,.,.,. @ 5
R(e[_],r[.]),R(e[_],r[.]),R(e[_],r[.]),R(e[_],r[.]),B(),B(),. <-> R(e[_],r[1]),R(e[_],r[1]),R(e[_],r[2]),R(e[_],r[2]),B(),B(),B() @ 7,8
E(r[.]),R(e[.]),.,. -> E(r[1]),R(e[1]),R(e[2]),E(r[2]) @ 1
E(r[.]),E(r[.]),. -> E(r[.]),E(r[1]),R(e[1],r[.]) @ 1
E(r[.]),E(r[.]),R(e[.],r[.]),. -> E(r[2]),E(r[1]),R(e[1],r[.]),R(e[2],r[.]) @ 1
E(r[.]),E(r[.]),.,. -> E(r[2]),E(r[1]),R(e[1],r[.]),R(e[2],r[.]) @ 1
%obs: 'dimmers' |R(r[_])|/2
%obs: 'monomers' |R(r[.])|

%init: 10 E(r[.])
%init: 100 R(r[.],e[.])
%init: 4.5 A()
%init: 6 B()
